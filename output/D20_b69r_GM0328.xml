            <pb xml:id="b69r" corresp="#contents_b69r #folDesc_b69rv #addDesc_b69r #N_black2" facs="#D-20b_65" change="#GM03completion #GM03finalVersion #proofreadingCb12Titel"/>
            <fw type="pageNumber" hand="#N_pencil" place="top-left" xml:id="b69r_fw_d2e26">65</fw>
            <fw type="pageNumber" hand="#N-Archiv_red" place="top-right" xml:id="b69r_fw_d2e29"><del rend="erased">101</del></fw>  
            <fw type="folNumber" hand="#GSA_pencil" place="bottom-left" xml:id="b69r_fw_d2e33">69</fw>
            <substJoin n="b69r,10-12" target="#substAddSeg1_b69r #substDel1a_b69r #substDel1b_b69r"/>
