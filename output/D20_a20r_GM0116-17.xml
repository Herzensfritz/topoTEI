            <fw type="pageNumber" hand="#N-Archiv_red" place="top-right" xml:id="a20r_fw_d2e29"><subst xml:id="a20r_subst_d2e30"><del rend="overwritten"><del rend="erased">17</del></del><add place="superimposed" xml:id="a20r_add_d2e34">14.</add></subst></fw>
