            <substJoin n="5-6" target="#subst1a_a20r #subst1b_a20r #subst1c_a20r #subst1d_a20r #substAdd1_a20r #substDel1_a20r"/>
            <substJoin n="11-12" target="#substAdd2_a20r #substDel2_a20r"/>
