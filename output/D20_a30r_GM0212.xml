            <substJoin n="a30r,22" target="#substAdd8_a30r #substDel8_a30r"/>
            <substJoin n="a30r,25-26" target="#subst9_a30r #substAdd9_a30r"/>
            <substJoin n="a30r,25-26" target="#subst10_a30r #substAdd10_a30r #substDel10a_a30r #substDel10b_a30r"/>
            <substJoin n="a30r,25-26" target="#substAdd11_a30r #substDel11a_a30r #substDel11b_a30r"/>
