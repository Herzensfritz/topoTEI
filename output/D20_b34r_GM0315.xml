            <app loc="b34r,1-2" corresp="#appAnchor_b34r2 #b34r_lb1 #b34r_lb2">
               <lem>bringt … dieser</lem>
               <note type="editorial" xml:id="b34r_note_d2e40">Abfolge der Textänderungen teils unklar</note>
            </app>
            <substJoin n="b34r,1" target="#subst1_b34r #substDel1_b34r"/>
